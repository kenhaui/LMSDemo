<div class="border-form-div">
<h2>New Classroom</h2>
<%= form_for([@course ,@course.classrooms.build]) do |f| %>
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :limit %><br>
    <%= f.number_field :limit %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
</div>
